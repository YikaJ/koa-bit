extends public/layout
block content
    include search/search-header
    div.container.clearfix
        div.leftContent
            //知识图谱
            if queryNum == 1
                div.panel.graph
                    div.panel-title 知识图谱
                    div.panel-content.force-graph
            //网页搜索
            div.panel
                div.panel-title 网页搜索
                div.panel-content
